&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;&lt;span style="color:#75715e"&gt;# 오늘 포함 과거 5일 종가 중 최저값&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;d[&lt;span style="color:#e6db74"&gt;&amp;#39;5d_min&amp;#39;&lt;/span&gt;] &lt;span style="color:#f92672"&gt;=&lt;/span&gt; d&lt;span style="color:#f92672"&gt;.&lt;/span&gt;rolling(&lt;span style="color:#ae81ff"&gt;5&lt;/span&gt;)[&lt;span style="color:#e6db74"&gt;&amp;#39;close&amp;#39;&lt;/span&gt;]&lt;span style="color:#f92672"&gt;.&lt;/span&gt;min() 
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;&lt;span style="color:#75715e"&gt;# 전일 종가&lt;/span&gt;
